<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="cdnjs-pdf-js" type="host">cdnjs.cloudflare.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="cdnjs-pdf-js-style" type="host">cdnjs.cloudflare.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="cdnjs-pdf-js-connect" type="host">cdnjs.cloudflare.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="cdnjs-pdf-js-font" type="host">cdnjs.cloudflare.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="cdnjs-pdf-js-img" type="host">cdnjs.cloudflare.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
